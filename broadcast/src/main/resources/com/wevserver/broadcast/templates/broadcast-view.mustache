<!DOCTYPE html>
<html lang="en">
    {{>com/wevserver/ui/templates/head-partial}}
    {{>com/wevserver/ui/templates/header-partial}}
    <body>
        <section class="section content">
            <dl>
                <div class="block">
                    <dt class="label">{{#t}}id{{/t}}</dt>
                    <dd>{{broadcast.id}}</dd>
                </div>
                <div class="block">
                    <dt class="label">{{#t}}version{{/t}}</dt>
                    <dd>{{broadcast.version}}</dd>
                </div>
                <div class="block">
                    <dt class="label">{{#t}}name{{/t}}</dt>
                    <dd>{{broadcast.name}}</dd>
                </div>
                <div class="block">
                    <dt class="label">{{#t}}url{{/t}}</dt>
                    <dd>{{broadcast.url}}</dd>
                </div>
                <div class="block">
                    <dt class="label">{{#t}}updated-at{{/t}}</dt>
                    <dd>{{broadcast.updatedAt}}</dd>
                </div>
                <div class="block">
                    <dt class="label">{{#t}}updated-by{{/t}}</dt>
                    <dd>{{broadcast.updatedBy}}</dd>
                </div>
            </dl>
        </section>
        <section class="section content">
            <h2 class="title is-2">{{#t}}actions{{/t}}</h2>
            <div class="buttons">
                {{#actions}}
                <a class="button is-dark" href="{{.}}" text="{{.}}">{{.}}</a>
                {{/actions}}
            </div>
        </section>
        <section class="section content">
            <h2 class="title is-2">{{#t}}integrations{{/t}}</h2>
            <div class="buttons">
                {{#integrations}}
                <a class="button is-dark" href="{{.}}">{{.}}</a>
                {{/integrations}}
            </div>
        </section>
    </body>
    {{>com/wevserver/ui/templates/footer-partial}}
</html>
